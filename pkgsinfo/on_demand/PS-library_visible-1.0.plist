	<key>preinstall_script</key>
	<string>#!/bin/bash
# Set the Variables
USERLOC="/Users"
THEUSER=$(who | awk 'NR==1 {print $1}' )
USERPATH="${USERLOC}/${THEUSER}"
LIBPATH="${USERPATH}/Library"
#Make the ~/Library folder visible
/usr/bin/chflags nohidden $LIBPATH
/usr/bin/open $LIBPATH
exit
</string>
